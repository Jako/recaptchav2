{
    "name": "ReCaptchaV2"
    ,"lowCaseName": "recaptchav2"
    ,"description": "ReCaptchaV2 integrates Version 2 of Google's ReCaptcha service into MODX as a FormIt hook."
    ,"author": "YJ Tso @sepiariver"
    ,"version": "0.9"
    ,"package":{
        "elements": {
            "snippets": [{
                "name": "recaptchav2"
            }, {
                "name": "recaptchav2_render"
            }]
            ,"chunks": [{
                "name": "recaptchav2_html"
              }, {
                "name": "sample_formit_contact_form"
            }]
        }
        ,"systemSettings": [{
            "key": "core_path"
            ,"name": "ReCaptcha V2 Core Path"
            ,"value": "{core_path}components/recaptchav2/"
        }, {
            "key": "secret_key"
            ,"name": "ReCaptcha V2 Secret Key"
            ,"value": ""
        }, {
            "key": "site_key"
            ,"name": "ReCaptcha V2 Site Key"
            ,"value": ""
        }]
    }
    
}